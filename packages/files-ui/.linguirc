{
   "catalogs": [{
      "path": "<rootDir>/src/locales/{locale}/messages",
      "include": ["<rootDir>/src/**"],
      "exclude": ["**/node_modules/**", "<rootDir>/src/locales/**"]
   }],
   "compileNamespace": "cjs",
   "format": "po",
   "formatOptions": { 
      "origins": false
   },
   "locales": ["de", "en", "fr", "no"],
   "sourceLocale": "en"
} 